  	<url-pattern>/GetMailServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SpellerServlet</servlet-name>
  	<url-pattern>/SpellerServlet</url-pattern>
  </servlet-mapping>
